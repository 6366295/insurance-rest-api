{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "TestProxy": {
            "matchCondition": {
                "route": "/"
            },
            "responseOverrides": {
                "response.statusCode": "404",
                "response.statusReason": "Not Found",
                "response.body": "{\"path\" : \"\", \"details\" : [\"Not Found\"]}"
            }
        }
    }
}